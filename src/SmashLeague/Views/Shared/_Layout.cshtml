<!DOCTYPE html>
<html ng-app="SmashLeague">
<head>
  <meta name="viewport" content="width=device-width" />
  <title>SMASH League</title>

  <environment names="Development">
    <link rel="stylesheet" href="~/lib/font-awesome/css/font-awesome.css" />
    <link rel="stylesheet" href="~/lib/octicons/octicons.css" />
    <link rel="stylesheet" href="~/lib/icon-battlenet/icon-battlenet.css" />
    <link rel="stylesheet" href="~/css/smash-league.css" />
  </environment>

</head>
<body style="padding-top: 50px">

  <div>
    @RenderBody()
  </div>

  <environment names="Development">
    <script src="~/lib/jquery/js/jquery.js"></script>
    <script src="~/lib/bootstrap/js/bootstrap.js"></script>
    <script src="~/lib/angular/js/angular.js"></script>
    <script src="~/lib/angular-ui-router/js/angular-ui-router.js"></script>
    <script src="~/js/smash-league.js"></script>
  </environment>

  @RenderSection("scripts")
</body>
</html>
